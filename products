    <script>
        window.siteLayer = {
            'site': {
                'version': 1,
                'brand': "tweed",
                'lastUpdated': "" // TODO
            },
            'page': {
                'name': "Our Cannabis Products | Tweed",
                'url': "https:\/\/tweed.com\/en\/products",
                'referrer': document.referrer,
                'language': "en"
            },
            'event': {}, // TODO
            'user': {},
            'product': {},
            'cart': []
        };

        window.promise_to_have_user_data = new Promise(function (resolve, reject) {
            var xhr = new XMLHttpRequest();
            xhr.onreadystatechange = function() {
                if (xhr.readyState == XMLHttpRequest.DONE) {
                    var data = JSON.parse(xhr.responseText);
                    resolve(data);
                }
            };
            xhr.open('GET', '/bin/tweed/whoami.json', true);
            xhr.send(null);
        });

        window.checkout_available = null;
        var checkout_ready = new Promise(function(resolve, reject){
            window.checkout_available = {resolve: resolve, reject: reject};
        });

        Promise.all([checkout_ready, promise_to_have_user_data]).then(function(results) {
            checkout = results[0];
            whoami = results[1];
            quantity = 0;

            window.siteLayer['user']['authState'] = !!whoami['username'];
            window.siteLayer['user']['province'] = getCookieValue("rw-age-gate_province");

            checkout.lineItems && checkout.lineItems.forEach(function (item) {
                window.siteLayer['cart'].push({
                    "name": item.title,
                    "price": "$" + item.variant.price,
                    "category": "",    // TODO
                    "description": "", // TODO
                    "sku": item.variant.sku,
                    "quantity": item.quantity
                })
            });

            try {
                var src = document.getElementById("adobedtm").getAttribute("data-src");

                if (src) {
                    var head = document.getElementsByTagName('head')[0];
                    var script = document.createElement('script');
                    script.type = 'text/javascript';
                    script.onload = function () {
                        _satellite.pageBottom();
                    };
                    script.src = src;
                    head.appendChild(script);
                }
            } catch(e) {
                console.warn("Analytics is not configured.")
            }
        });
    </script>

